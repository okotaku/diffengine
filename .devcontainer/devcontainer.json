{
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "diffengine",
  "workspaceFolder": "/workspace",
  "postCreateCommand": "pre-commit install",
  "customizations": {
    "vscode": {
      "extensions": [
        "njpwerner.autodocstring",
